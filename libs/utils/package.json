{
  "name": "@shared/utils",
  "version": "1.0.0",
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "prebuild": "rimraf ./dist",
    "build": "run-s compile",
    "postbuild": "node ./scripts/prepare-package-json.js",
    "test": "jest"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
