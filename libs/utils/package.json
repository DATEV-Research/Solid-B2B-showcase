      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "hackathon-demo": "github:mandat-project/hackathon-demo#353-extract-auth-app",
    "n3": "^1.23.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.53.0",
    "vue": "3.4.27",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@types/n3": "^1.21.1",
    "@types/node": "^22.9.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.7.2"
  }
}
